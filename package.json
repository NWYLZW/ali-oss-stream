{
  "name": "ali-oss-stream",
  "version": "1.0.0",
  "description": "ali oss 服务流式操作",
  "author": "yijie",
  "license": "MIT",
  "main": "lib/index.umd.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "api": "api-extractor run",
    "dev": "rollup -w -c --environment FORMAT:esm",
    "build": "rimraf lib/* && run-p build:esm build:umd build:min build:dec",
    "build:esm": "rollup -c --environment FORMAT:esm",
    "build:umd": "rollup -c --environment FORMAT:umd",
    "build:min": "rollup -c --environment FORMAT:min",
    "build:dec": "tsc -p . --declaration --emitDeclarationOnly",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "test": "jest --coverage --verbose -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & jest -u"
    }
  },
  "lint-staged": {
    "*./src/**/*.{js,ts,json,md}": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@microsoft/api-extractor": "^7.13.2",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-jest": "^24.3.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash.merge": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
